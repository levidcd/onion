{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "rootDir": "./src",
    "skipLibCheck": true,
    "outDir": "es",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"]
    },
    "noImplicitAny": false
  },
  "include": ["src/**/*"],
  "exclude": ["jest.config.js"]
}
